<Window x:Class="PRB.PE01.Reservatiesysteem.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PRB.PE01.Reservatiesysteem"
        mc:Ignorable="d"
        Title="MainWindow" Height="474" Width="825" ResizeMode="NoResize">
    <Grid>
        <ComboBox x:Name="EindLst" HorizontalAlignment="Left" Height="27" Margin="185,48,0,0" VerticalAlignment="Top" Width="140">
            <ComboBoxItem Content="8"/>
            <ComboBoxItem Content="9"/>
            <ComboBoxItem Content="10"/>
            <ComboBoxItem Content="11"/>
            <ComboBoxItem Content="12"/>
            <ComboBoxItem Content="13"/>
            <ComboBoxItem Content="14"/>
            <ComboBoxItem Content="15"/>
            <ComboBoxItem Content="16"/>
            <ComboBoxItem Content="17"/>
        </ComboBox>
        <TextBox x:Name="NaamTxt" HorizontalAlignment="Left" Height="27" Margin="343,48,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="129"/>
        <TextBox x:Name="KortingTxt" HorizontalAlignment="Left" Height="27" Margin="500,48,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="128"/>
        <Button x:Name="ResBtn" Content="Reserveer" HorizontalAlignment="Left" Height="27" Margin="649,48,0,0" VerticalAlignment="Top" Width="108" Click="ResBtn_Click"/>
        <ComboBox x:Name="BeginLst" HorizontalAlignment="Left" Height="27" Margin="21,48,0,0" VerticalAlignment="Top" Width="140" SelectionChanged="BeginLst_SelectionChanged">
